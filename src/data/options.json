{
    "$schema": "https://raw.githubusercontent.com/ManualForArchipelago/Manual/main/schemas/Manual.options.schema.json",
    "_comment": "Add a _ before an option name to comment it out and it wont be added to the apworld",
    "data": {
        "_commented_out_option":{
            "type": "DefaultOnToggle",
            "_comment": ["Options, whose name start with _ won't be used in manual"]
        },
        "_Toggle":{
            "description": ["this is a Toggle","A simple boolean, the default value is False. Use DefaultOnToggle instead for True"],
            "type": "Toggle",
            "group": "Example Options"
        },
        "_DefaultOnToggle":{
            "description": ["this is a DefaultOnToggle","A simple boolean, the default value is True. Use Toggle instead for False"],
            "type": "DefaultOnToggle",
            "group": "Example Options"
        },
        "_Choice":{
            "type": "Choice",
            "description": ["This is a Choice", "Let the user pick from a list of values", "Currently the only use for choice in Manual is to modify the goal"],
            "values": {
                "start":0,
                "test":1
             },
            "default": 0,
            "group": "Example Options"
        },
        "_TextChoice":{
            "type":"TextChoice",
            "description": ["Currently Disabled in Manual's Option.json", "This is a TextChoice", "Like Choice but the player can define their own values"],
            "values": {
                "start":0,
                "test":1
             },
            "default": 0,
            "group": "Example Options"
        },
        "_Range":{
            "type": "Range",
            "description": ["Currently Disabled in Manual's Option.json", "this is a Range","Allow the player to specify a value between 'start' and 'end'"],
            "range_start": 0,
            "default": 1,
            "range_end": 10,
            "group": "Example Options"
        },
        "_NamedRange":{
            "description": ["Currently Disabled in Manual's Option.json","this is a NamedRange","Like a Range, but with your own defined label for certain values"],
            "type": "NamedRange",
            "range_start": 0,
            "range_end": 10,
            "special_range_names": {"test":2},
            "group": "Example Options"
        },

        "DLC_enabled":{
            "display_name": "DLC Enabled",
            "description": ["Is the Dlc enabled?"],
            "type": "DefaultOnToggle"
        },
        "goal":{
            "type": "Choice",
            "display_name": "Goal",
            "description": ["How to End your Randomized playthrough."],
            "values": {},
            "_comment": "when modifying the goal leave the values empty since they'll be generated by manual"
        }
    }

}